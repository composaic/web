{
    "name": "@composaic/web",
    "version": "0.0.5",
    "description": "Common plugins for web development",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "scripts": {
        "build:cjs": "BABEL_ENV=cjs babel src -d dist/cjs --extensions '.ts,.tsx' --source-maps",
        "build:esm": "BABEL_ENV=esm babel src -d dist/esm --extensions '.ts,.tsx' --source-maps",
        "build:types": "tsc --emitDeclarationOnly --declaration -p tsconfig.base.json --outDir dist/types",
        "build:scss": "sass src/:dist/esm/ --no-source-map && sass src/:dist/cjs/ --no-source-map",
        "build": "rm -rf dist && npm run build:cjs && npm run build:esm && npm run build:types && npm run build:scss",
        "clean": "rimraf dist",
        "lint": "eslint 'src/**/*.{ts,tsx}'",
        "test": "jest"
    },
    "author": "Johnny Jarecsni <johnny.jarecsni@icloud.com>",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.21.5",
        "@babel/core": "^7.21.5",
        "@babel/plugin-transform-runtime": "^7.21.5",
        "@babel/preset-env": "^7.21.5",
        "@babel/preset-react": "^7.21.5",
        "@babel/preset-typescript": "^7.21.5",
        "@babel/runtime": "^7.21.5",
        "@types/jest": "^29.5.12",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "babel-loader": "^9.1.2",
        "babel-plugin-transform-import-extension": "^1.0.3",
        "concurrently": "^8.2.2",
        "css-loader": "^6.11.0",
        "eslint": "^9.10.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "jest": "^29.5.0",
        "mini-css-extract-plugin": "^2.9.2",
        "prettier": "^3.3.1",
        "rimraf": "^6.0.1",
        "sass": "^1.83.4",
        "sass-loader": "^13.3.3",
        "style-loader": "^3.3.3",
        "typescript": "^5.6.2",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4",
        "webpack-node-externals": "^3.0.0"
    },
    "dependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "jsdom",
        "moduleNameMapper": {
            "\\.(css|scss)$": "identity-obj-proxy"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/composaic/web.git"
    },
    "keywords": [
        "composaic",
        "web",
        "plugins"
    ],
    "bugs": {
        "url": "https://github.com/composaic/web/issues"
    },
    "homepage": "https://github.com/composaic/web#readme"
}